{
  "name": "sobrecitos",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "build-no-errors": "tsc ; vite build",
    "build-with-types": "tsc --noEmit && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "types:supabase": "npx supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/supabase.ts",
    "check:supabase": "node check-supabase-config.js",
    "migrate:supabase": "node deploy-migrations.js",
    "setup:supabase": "npm run check:supabase && npm run migrate:supabase && npm run types:supabase",
    "supabase:connect": "node supabase-login.js",
    "clean:duplicates": "node clean-duplicates.js",
    "clean:duplicates:confirm": "node clean-duplicates.js --confirm --confirm-db"
  },
